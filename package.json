{
  "name": "sass-a11ycolor",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "watch": "npm run watch:css | npm run watch:site",
    "watch:site": "browser-sync start --server --files \"css/*.css\" \"*.html\"",
    "watch:css": "node-sass -w --source-map true --output-style compressed sass/main.scss css/style.css",
    "build": "node-sass --source-map true --output-style compressed sass/main.scss css/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alex-page/sass-a11ycolor.git"
  },
  "keywords": [
    "sass",
    "accessible",
    "colour",
    "color"
  ],
